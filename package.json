{
  "name": "e-commerce-admin-ui",
  "version": "0.0.0",
  "description": "Admin User Interface for the E-Commerce domain",
  "scripts": {
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "build": "npm run web-bundle;npm run build-web",
    "build-web": "docker build --tag erpmicroservices/e-commerce-admin-service --rm .",
    "clear-cache": "gulp clear-cache",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "start": "npm run start-native; npm  run start-web",
    "start-native": "react-native start",
    "start-web": "docker-compose up",
    "test": "cucumber-js --tags @admin-ui --compiler es6:babel-core/register -r ./features ../e-commerce-features",
    "test-one": "cucumber-js --name \"A web preference list can be produced\" --compiler es6:babel-core/register -r ./features ../e-commerce-features",
    "web-bundle": "webpack --config web/webpack/web.prod.config.js --progress --colors",
    "web-dev": "webpack --progress --colors --watch -d --config web/webpack/web.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErpMicroServices/e-commerce-admin-ui.git"
  },
  "keywords": [
    "e-commerce",
    "admin",
    "erp",
    "content"
  ],
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ErpMicroServices/e-commerce-admin-ui/issues"
  },
  "homepage": "https://github.com/ErpMicroServices/e-commerce-admin-ui#readme",
  "dependencies": {
    "apollo-client": "^1.0.0-rc.2",
    "bootstrap-react-components": "^0.16.0",
    "graphql-tag": "^1.3.1",
    "jsonwebtoken": "^7.3.0",
    "jwt-decode": "^2.1.0",
    "react": "15.4.2",
    "react-apollo": "^1.0.0-rc.2",
    "react-dom": "^15.4.2",
    "react-native": "0.42.0",
    "react-redux": "^4.4.6",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.8",
    "react-templates-and-utils": "^0.3.4",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-devtools": "^3.3.2",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "cucumber": "^2.0.0-rc.7",
    "eslint": "^3.17.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^6.10.0",
    "jest": "19.0.2",
    "jsx-html-class": "^1.0.3",
    "react-test-renderer": "15.4.2",
    "selenium-webdriver": "^3.3.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1"
  }
}
