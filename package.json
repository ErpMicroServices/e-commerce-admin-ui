{
    "name": "e-commerce-admin-ui",
    "version": "0.0.0",
    "description": "Admin User Interface for the E-Commerce domain",
    "scripts": {
        "android:bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
        "android:bundle:dev": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
        "build": "npm run build:bundle;npm run build:web",
        "build:web": "docker build --tag erpmicroservices/e-commerce-admin-ui --rm .",
        "build:bundle": "webpack --config web/webpack/web.prod.config.js --progress --colors",
        "clear:cache": "gulp clear-cache",
        "db_admin": "docker run --detach --publish 5050:5050 --name e-commerce-admin-ui-pgadmin --network=ecommerceadminui_default --link e-commerce-admin-ui-db:e-commerce-db thajeztah/pgadmin4",
        "ios:bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
        "ios:bundle:dev": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
        "push": "docker push erpmicroservices/e-commerce-admin-ui:latest",
        "start": "npm run start:native; npm  run start:web",
        "start:native": "react-native start",
        "start:web": "docker-compose up",
        "test": "cucumber-js --tags @admin-ui --compiler es6:babel-core/register -r ./features ../e-commerce-features",
        "web:dev": "webpack --progress --colors --watch -d --config web/webpack/web.dev.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ErpMicroServices/e-commerce-admin-ui.git"
    },
    "keywords": [
        "e-commerce",
        "admin",
        "erp",
        "content"
    ],
    "author": {
        "name": "Jim Barrows",
        "email": "Jim.Barrows@gmail.com"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/ErpMicroServices/e-commerce-admin-ui/issues"
    },
    "homepage": "https://github.com/ErpMicroServices/e-commerce-admin-ui#readme",
    "dependencies": {
        "apollo-client": "^2.6.10",
        "bootstrap-react-components": "^0.16.0",
        "graphql-tag": "^1.3.1",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^3.1.2",
        "react": "^17.0.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^17.0.1",
        "react-native": "^0.63.4",
        "react-redux": "^4.4.6",
        "react-router": "^2.5.2",
        "react-router-redux": "^4.0.8",
        "react-templates-and-utils": "^0.4.1",
        "redux": "^4.0.5",
        "redux-auth-wrapper": "^3.0.0",
        "redux-devtools": "^3.3.2",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "babel-cli": "^6.24.0",
        "babel-core": "^6.24.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "babel-plugin-transform-class-properties": "^6.23.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-react": "^6.23.0",
        "babel-preset-react-native": "^4.0.1",
        "babel-preset-stage-2": "^6.22.0",
        "babel-register": "^6.24.0",
        "chai": "^4.3.0",
        "@cucumber/cucumber": "^7.0.0-rc.0",
        "eslint": "^7.20.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-react": "^7.22.0",
        "jest": "^26.6.3",
        "jsx-html-class": "^1.0.3",
        "react-test-renderer": "^17.0.1",
        "selenium-webdriver": "^4.0.0-beta.1",
        "webpack": "^5.23.0",
        "webpack-dev-middleware": "^4.1.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-hot-middleware": "^2.17.1"
    }
}
